<!DOCTYPE html>
<html>
    <head>
<!-- CSS only -->
<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.0/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-KyZXEAg3QhqLMpG8r+8fhAXLRk2vvoC2f3B09zVXn8CA5QIVfZOJ3BCsw2P0p/We" crossorigin="anonymous">        
		<title>Super Market El Bahrin| Products</title>
        <link href="./products.css" rel="stylesheet">
        <link rel="preconnect" href="https://fonts.googleapis.com">
<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
<link href="https://fonts.googleapis.com/css2?family=Roboto:wght@100&display=swap" rel="stylesheet">
    </head>
    <body>
		<nav aria-label="breadcrumb">
  <ol class="breadcrumb">
    <li class="breadcrumb-item"><a href="/categories">Home</a></li>
    <li class="breadcrumb-item active" aria-current="page"><%= category %></li>
  </ol>
</nav>
	<div class="container-flex">
		<p class="category" style="display: none"><%= category %></p>
		<% products.forEach(product => { %>
<div style="margin-top: 25px;">
<div class="card">
    <div class="header">
		<img class="header-image" src="data:image/png;base64,">
		<p class="img" style="display: none;"><%= product.image %></p>
    </div>
    <div class="body">
        <h3 class="name"><%= product.name %></h3>
        <div class="input-group mb-3">
            <label class="input-group-text" for="inputGroupSelect01">Taste</label>
            <select class="form-select" id="inputGroupSelect01">
              <option selected>Choose...</option>
			<% for(var i = 0; i < product.varaity.length; i++){ %>
				<%= product.varaity[i].varaity %>
              <option value="<%= product.varaity[i].stock %>"><%= product.varaity[i].varaity %></option>
			<% } %>
            </select>
          </div>
	<div class="size-contain" id=<%= product.name %>>	
		<p class="chosenSize" style="display: none"></p>
		<h3>Size: </h3>
		<div class="size-container">
			<p class="sizeArr" style="display: none"><%= product.size %></p>
			<p class="stockArr" style="display: none"><%= product.stock %></p>
			<p class="size" style="display: none"><%= product._id %></p>
			<!-- <div class="form-check">
				<input class="form-check-input radios" checked type="radio">
				<label class="form-check-label" for="flexRadioDefault1">
				</label>
			  </div> -->
		</div>
	</div>
    </div>
    </div>
    <div class="numpad" style="margin-bottom: 5px;">
        <button class="delete btn"><img class="add-img" src="negative.png"></button>
        <div class="num">0</div>
        <button class="add btn"><img class="add-img" src="./plus.png"></button>
    </div>
    <button class="add-to-cart btn-outline-success btn"><img style="width: 30px; height: 30px;" src="./add-to-cart.png"> Add To Cart</button>
    </div>
		<% }) %>
		<a class="float" href="/checkout">
			<img href="/checkout" class="icon float" style="padding: 10px;" src="./cart.png">
		</a>			
		</div>
		<script src='products.js'></script>
		<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.6.0/jquery.min.js" integrity="sha512-894YE6QWD5I59HgZOGReFYm4dnWc1Qt5NtvYSaNcOP+u1T9qYdvdihz0PPSiiqn/+/3e7Jo4EaG7TubfWGUrMQ==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>
		<script src="https://d3js.org/d3.v7.min.js"></script>
    </body>
</html>